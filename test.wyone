term And({Term})
term Or({Term})
term Not(Term)
term Var(string)
term True
term False

class Bool as True | False
class Term as And | Or | Not | Bool | Var

rewrite Not(Bool t):
    => True, if t == False
    => False

rewrite And({Term} xs):
    => let ys = False in ys, if False in xs
    => True, if xs == {True}

rewrite Or({Term} xs):
    => True, if True in xs
    => False, if xs == {False}
