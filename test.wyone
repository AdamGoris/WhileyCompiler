term And({Term})
term Or({Term})
term Not(Term)
term True
term False

class Bool as True | False

class Term as And | Or | Not | Bool

rewrite Not(Bool t):
    => True, if t == False
    => False

rewrite And({Term} xs):
    => True, if True in xs
    => False, if xs == {False}

rewrite Or({Term} xs):
    => False, if False in xs
    => True, if xs == {True}
