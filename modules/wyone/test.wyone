term False
term True
class Bool as True | False

term Not(Term)
term And{Term...}
term Or{Term...}
term Var(string)
class Term as Not | And | Or | Var | Bool

rewrite And{And{Term... xs}, Term... ys}:
    => And (xs ++ ys)

rewrite Not(And{Term... xs}):
    => let ys = { Not(x) | x in xs }
       in Or(ys)








