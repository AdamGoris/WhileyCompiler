define invariant([int] items, int n) is:
    forall (int i):
        if:
            i < n
        then:
            items[i] == 0

assert "loop invariant not restored":
    forall ([int] items, int i):
        if:
            invariant(items, i)
        then:
            invariant(items[i:=0], i + 1)


