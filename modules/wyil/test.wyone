term True
term False
class Bool as True | False

term Not(Term)
term Var(string)
term And[Term...]
term LessThan[Term,Term]

rewrite Not(True x):
    => False

rewrite Not(False x):
    => True

rewrite Not(Not(* x)):
    => x

rewrite And[LessThan[* x,* y] l1, LessThan[* y,* z] l2]:
    => And[l1,l2,LessThan[x,z]]

class Term as Bool | Not | Var






