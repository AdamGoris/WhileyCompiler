term True
term False
class Bool as True | False
term Not(Term)
term Var(String)

rewrite Not(True x):
    => False

rewrite Not(False x):
    => True

rewrite Not(Not(* x)):
    => x

class Term as Bool | Not | Var

term Sum[Num,Num]
term Num(int)

rewrite Sum[Num(int x), Num(int y)]:
    => Num(x/y)






